import pandas as pd

# Load the data from a file (e.g., CSV)
df = pd.read_csv('your_file.csv')

# Drop duplicates based on a key field (e.g., 'key_column')
df_non_duplicate = df.drop_duplicates(subset='key_column', keep='first')

# Save the non-duplicate records to a new file
df_non_duplicate.to_csv('non_duplicate_records.csv', index=False)