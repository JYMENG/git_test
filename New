MonthEndDates = 
VAR StartDate = DATE(2020, 1, 1)  // Adjust as needed
VAR EndDate = DATE(2030, 12, 31)  // Adjust as needed
RETURN
    DISTINCT(
        SELECTCOLUMNS(
            ADDCOLUMNS(
                CALENDAR(StartDate, EndDate),
                "MonthEndDate", EOMONTH([Date], 0)
            ),
            "MonthEndDate", [MonthEndDate]
        )
    )