/* Define the path to the Excel file */
%let excel_file = C:\path\to\your\file.xlsx;

/* List of sheet names manually specified */
%let sheet_names = Sheet1 Sheet2 Sheet3; /* Add all known sheet names here */

/* Create a macro to import each sheet */
%macro import_sheets(file=, sheets=);
    %let num_sheets = %sysfunc(countw(&sheets));
    
    %do i = 1 %to &num_sheets;
        %let sheet_name = %scan(&sheets, &i);
        
        proc import datafile="&file"
            out=sheet_&i
            dbms=xlsx replace;
            sheet="&sheet_name";
            getnames=yes;
        run;
    %end;
%mend import_sheets;

/* Call the macro to import the sheets */
%import_sheets(file=&excel_file, sheets=&sheet_names);