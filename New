# Install if not present
Install-Module AzureAD

# Connect
Connect-AzureAD

# Get group + members
$group = Get-AzureADGroup -SearchString "MyGroupName"
Get-AzureADGroupMember -ObjectId $group.ObjectId |
    Select-Object @{Name='GroupName';Expression={$group.DisplayName}}, DisplayName, UserPrincipalName