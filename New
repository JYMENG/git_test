SELECT record_date, col1, col2
FROM (
  SELECT
    record_date,
    col1, col2,
    LAG(col1) OVER (ORDER BY record_date) AS prev_col1,
    LAG(col2) OVER (ORDER BY record_date) AS prev_col2
  FROM dbo.daily_data
) s
WHERE
  (col1 <> prev_col1 OR (col1 IS NULL) <> (prev_col1 IS NULL))
  OR
  (col2 <> prev_col2 OR (col2 IS NULL) <> (prev_col2 IS NULL));