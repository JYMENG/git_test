import os
import re
from datetime import datetime

# Folder path
folder_path = 'your/folder/path'

# Regex to extract mm-dd-yyyy
date_pattern = re.compile(r'(\d{2})-(\d{2})-(\d{4})')

def extract_date_from_filename(filename):
    match = date_pattern.search(filename)
    if match:
        mm, dd, yyyy = match.groups()
        return datetime.strptime(f"{mm}-{dd}-{yyyy}", "%m-%d-%Y")
    return None

# List and sort files with valid dates
files_with_dates = []
for file in os.listdir(folder_path):
    date = extract_date_from_filename(file)
    if date:
        files_with_dates.append((file, date))

# Sort by date
files_with_dates.sort(key=lambda x: x[1])

# Get sorted filenames
sorted_files = [file for file, _ in files_with_dates]

# Example: print sorted filenames
for f in sorted_files:
    print(f)